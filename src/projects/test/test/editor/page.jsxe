<Oak-Page id="editor" title="Editor Package Test" style={{ padding: "1em" }}>
  <h2>Editor Test</h2>
  <Editor-Form
    data={data}

    schema={{
      foo: {
        required: true
      }
    }}

    errors={{
      foo:"NOOOO"
    }}

    controlProps={{
      inline: false,
      labelOn: "left",
      labelProps: {
        style: { width: 200 },
      }
    }}

    style={{
      border: "1px solid transparent"
    }}>

    <h5>Wrapped generic Control</h5>
    <Editor-Control name="foo" label="Foo:">
      <input/>
    </Editor-Control>

    <h5>Control Subclasses</h5>
    <Editor-Output name="foo" label="Output Field:"/>

    <Editor-Text name="foo" label="Text Field:" width="10"/>

    <Editor-Password name="foo" label="Password Field:" tabIndex="20"
      error={["Password doesn't match", "Password is not long enough"]}
      labelProps={{style:{color:"#999"}}} hint="At least 6 letters, numbers or special characters"/>

    <br/>
    <Editor-Select name="select" label="Select:" values={["yes","no"]} inline/>
    <Editor-Output name="select" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Select name="select" label="Required Select:" required values={{ yes: "YES", no: "NO" }}  inline/>
    <Editor-Output name="select" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Select name="booleanSelect" label="Boolean Select:" values={[true, false]} inline/>
    <Editor-Output name="booleanSelect" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Checkbox name="checker" label="Checkbox" inline error="NOOOOO" width={5}/>
    <Editor-Output name="checker" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Checkbox name="yesno" label="Yes/No Checkbox" trueValue="yes" falseValue="no" inline width={5}/>
    <Editor-Output name="yesno" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Checkbox name="trueUndefined" label="True/undefined Checkbox" falseValue={undefined} inline width={5}/>
    <Editor-Output name="trueUndefined" inline value={value => `(${typeof value}) ${value}`}/>

    <br/>
    <Editor-Group name="address" label="Address:">
      <Editor-Text name="street[0]" label="Street:"/>
      <Editor-Text name="street[1]" label=""/>
      <Editor-Text name="city" label="City:"/>
      <Editor-Text name="state" label="State:"/>
      <Editor-Text name="zip" label="Zip Code:"/>
    </Editor-Group>


  </Editor-Form>
</Oak-Page>
